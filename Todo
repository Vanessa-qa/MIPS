module MemI(
	input [31:0] Dir
	output reg [31:0] DatoL, //Dato de lectura

);

reg [7:0]mem[0:999]; //Memoria

initial
begin  
	#100;
	$readmemb("datos", inst.mem); //Leer en binario
end 

always @(*)
begin
	DatoL = mem[Dir];
end
endmodule


module Flan(
	input clk,
	output reg [31:0] Inst
);

wire [31:0] WIn, WRes;

PC PcInst(.data_in(WIn), .data_out(WRes), .CLK(clk));
Sumador suma(.DataIn(WRes), .Res(WIn));
